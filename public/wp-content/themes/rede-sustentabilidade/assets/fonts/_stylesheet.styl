@font-face {
    font-family: 'PT Sans';
    src: url('assets/fonts/PT-Sans-fontfacekit/ptsans_regular_macroman/PTS55F-webfont.eot');
    src: url('assets/fonts/PT-Sans-fontfacekit/ptsans_regular_macroman/PTS55F-webfont.eot?#iefix') format('embedded-opentype'),
         url('assets/fonts/PT-Sans-fontfacekit/ptsans_regular_macroman/PTS55F-webfont.woff') format('woff'),
         url('assets/fonts/PT-Sans-fontfacekit/ptsans_regular_macroman/PTS55F-webfont.ttf') format('truetype'),
         url('assets/fonts/PT-Sans-fontfacekit/ptsans_regular_macroman/PTS55F-webfont.svg#pt_sansregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

